{
  "ignition": {
    "config": {
      "replace": {
        "source": null,
        "verification": {}
      }
    },
    "security": {
      "tls": {}
    },
    "timeouts": {},
    "version": "3.0.0"
  },
  "passwd": {
    "users": [
      {
        "groups": [
          "sudo"
        ],
        "name": "maintuser",
        "sshAuthorizedKeys": [
          "ssh-rsa  maintuser@libvirt.local"
        ],
        "uid": 1001
      },
      {
        "groups": [
          "docker"
        ],
        "name": "jenkins",
        "sshAuthorizedKeys": [
          "ssh-rsa  jenkins@libvirt.local"
        ],
        "uid": 9999
      }
    ]
  },
  "storage": {
    "directories": [
      {
        "group": {
          "name": "root"
        },
        "path": "/var/lib/jenkins",
        "user": {
          "name": "jenkins"
        },
        "mode": 488
      },
      {
        "group": {
          "name": "root"
        },
        "path": "/var/lib/jenkins/agent",
        "user": {
          "name": "jenkins"
        },
        "mode": 488
      }
    ],
    "files": [
      {
        "group": {
          "name": "root"
        },
        "overwrite": true,
        "path": "/etc/hostname",
        "user": {
          "name": "root"
        },
        "contents": {
          "source": "data:,jenkins-slave.libvirt.local",
          "verification": {}
        },
        "mode": 420
      }
    ]
  },
  "systemd": {
    "units": [
      {
        "contents": "[Unit]\nDescription=Install Overlay Packages\nConditionFirstBoot=yes\nWants=network-online.target\nBefore=boot-complete.target\nAfter=network-online.target\nAfter=multi-user.target\n\n[Service]\nType=oneshot\nExecStart=rpm-ostree install java-1.8.0-openjdk\n# An alternative to --reboot\nExecStartPost=rpm-ostree ex livefs --i-like-danger\n\n[Install]\nRequiredBy=boot-complete.target\nWantedBy=multi-user.target",
        "enabled": true,
        "name": "overlay-install.service"
      }
    ]
  }
}